<launch>
  <master auto="start"/>

  <!-- Run Rviz -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find raspimouse_navigation_2)/config/rviz_config.rviz"/>

  <!-- Run Move Base -->
  <include file="$(find raspimouse_navigation_2)/launch/run_move_base.launch"/>

  <!-- Run Hector SLAM -->
  <include file="$(find hector_geotiff)/launch/geotiff_mapping.launch"/>
  <include file="$(find raspimouse_hector_slam)/launch/run_hector_mapper.launch"/>

</launch>
